package com.spiderlogic.model;

public class GuestbookMessage {

    int id;
    String text;

	public String getText() {
		return text;
	}

	public void setText(String text) {
		this.text = text;
	}

    public int getId() {
        return id;
    }

    public void setId(int id) {
        this.id = id;
    }

	public GuestbookMessage(int id, String text) {
        this.id = id;
		this.text = text;
	}

	public GuestbookMessage() {
        this.id = 0;
        this.text = "";
	}

}
